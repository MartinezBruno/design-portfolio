  <script is:inline>
    const drawerToggle = document.querySelectorAll(".drawerToggle");
    const drawer = document.querySelector(".drawer");

    drawerToggle.forEach((toggle) => {
      toggle.addEventListener("click", () => {
        drawer.classList.toggle("translate-x-[150%]");
      });
    });

    document.addEventListener("click", (event) => {
      const isClickInsideDrawer = drawer.contains(event.target);
      const isClickOnToggleButton = drawerToggle.contains(event.target);

      if (!isClickInsideDrawer && !isClickOnToggleButton) {
        if (!drawer.classList.contains("translate-x-[150%]")) {
          drawer.classList.add("translate-x-[150%]");
        }
      }
    });
  </script>
